package com.faubel.weather.entities;

import java.util.List;

public class OpenWeatherResponse {
	private double lat;
	private double lon;
	private String timezone;
	private int timezone_offset;
	private List<DailyWeather> daily;

	public double getLat() {
		return lat;
	}

	public void setLat(double lat) {
		this.lat = lat;
	}

	public double getLon() {
		return lon;
	}

	public void setLon(double lon) {
		this.lon = lon;
	}

	public String getTimezone() {
		return timezone;
	}

	public void setTimezone(String timezone) {
		this.timezone = timezone;
	}

	public int getTimezone_offset() {
		return timezone_offset;
	}

	public void setTimezone_offset(int timezone_offset) {
		this.timezone_offset = timezone_offset;
	}

	public List<DailyWeather> getDaily() {
		return daily;
	}

	public void setDaily(List<DailyWeather> daily) {
		this.daily = daily;
	}

	public List<DailyWeather> getDailyWeather() {
		return daily;
	}

	public void setDailyWeather(List<DailyWeather> daily) {
		this.daily = daily;
	}
}
